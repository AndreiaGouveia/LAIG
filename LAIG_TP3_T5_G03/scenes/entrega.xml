<lxs>
    <scene root="deliveryRoot" axis_length="5"/>

    <!-- Cameras -->
    <views default="defaultCamera">

         <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="-15" y="8" z="-15" />
            <to x="-5" y="9" z="-5" />
        </perspective>

    </views>

    <!-- Ambient Light-->
    <globals>
        <ambient r="0.5" g="0.5" b="0.6" a="1.0" />
        <background r="1" g="1" b="0.85" a="1.0" />
    </globals>

    <!-- Other Lights-->
    <lights>
        <omni id="mainLightOmni" enabled="0">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0.7" g="0.7" b="0.7" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1" linear="0" quadratic="0" />
        </omni>

    </lights>

    <textures>
    </textures>

    <materials>
        <material id="demoMaterial" shininess="10">
            <emission r="0.3" g="0.3" b="0.3" a="1" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
    </materials>

    <transformations>
    </transformations>

    <animations>

        
    </animations>


    <primitives>
        <primitive id="Rectangle">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
    </primitives>

    <components>

            <component id="deliveryRoot">
                <transformation>
                </transformation>
                <materials>
                    <material id="demoMaterial" />
                </materials>
                <texture id="none"/>
                <children>
                    <primitiveref id="Rectangle" />
                </children>
            </component>

        </components>
    </lxs>