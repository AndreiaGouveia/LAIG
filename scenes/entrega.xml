<lxs>
    <scene root="deliveryRoot" axis_length="5"/>

    <!-- Cameras -->
    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="120" y="50" z="120" />
            <to x="0" y="10" z="0" />
        </perspective>

        <perspective id="frontView" near="0.1" far="500" angle="45">
            <from x="1" y="2" z="-15" />
            <to x="0" y="6" z="-5" />
        </perspective>

        <perspective id="sideView" near="0.1" far="500" angle="45">
            <from x="80" y="10" z="40" />
            <to x="40" y="10" z="40" />
        </perspective>

        <perspective id="topView" near="0.1" far="500" angle="45">
            <from x="1" y="120" z="-50" />
            <to x="0" y="80" z="-25" />
        </perspective>

        <ortho id="orthoFrontCamera" near="0.1" far="500" left="-10" right="10" top="10" bottom="-10">
             <from x="1" y="3" z="-50" />
            <to x="0" y="5" z="-20" />
            <up x="0" y="1" z="0" />
            <!-- opcional, default 0,1,0 -->
        </ortho>

        <ortho id="orthoSideCamera" near="0.1" far="500" left="-10" right="10" top="10" bottom="-10">
           <from x="80" y="10" z="20" />
            <to x="10" y="10" z="25" />
            <up x="0" y="1" z="0" />
        </ortho>

        <ortho id="orthoTopCamera" near="0.1" far="500" left="-10" right="10" top="10" bottom="-10">
            <from x="1" y="80" z="-120" />
            <to x="0" y="45" z="-40" />
            <up x="0" y="1" z="0" />
        </ortho>
    </views>

    <!-- Ambient Light-->
    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <!-- Other Lights-->
    <lights>
        <omni id="mainLightOmni" enabled="0">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>

       <spot id="statueSpotFront" enabled="f" angle="1" exponent="1">
            <location x="2" y="2" z="15" w="1" />
            <target x="2" y="5" z="15" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </spot>

        <spot id="statueSpotBack" enabled="f" angle="1" exponent="1">
            <location x="2" y="2" z="18" w="1" />
            <target x="2" y="5" z="15" />
            <ambient r="0" g="0" b="0" a="1" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </spot>
    </lights>

